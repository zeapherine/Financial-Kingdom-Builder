# Production Environment Configuration
API_BASE_URL=https://api.financialkingdom.com
COINGECKO_API_KEY=your_prod_coingecko_api_key_here
EXTENDED_API_KEY=your_prod_extended_api_key_here
BRAVE_API_KEY=your_prod_brave_api_key_here

# Feature Flags
ENABLE_REAL_TRADING=true
ENABLE_SOCIAL_FEATURES=true
